{
  "sampling_rate": 512,
  "channel_mapping": {
    "ch0": {
      "sensor": "EMG",
      "enabled": true
    },
    "ch1": {
      "sensor": "EOG",
      "enabled": true
    }
  },
  "filters": {
    "EMG": {
      "type": "high_pass",
      "cutoff": 70.0,
      "order": 4
    },
    "EOG": {
      "type": "low_pass",
      "cutoff": 10.0,
      "order": 4
    },
    "EEG": {
      "filters": [
        {
          "type": "notch",
          "freq": 50.0,
          "Q": 30
        },
        {
          "type": "bandpass",
          "low": 0.5,
          "high": 45.0,
          "order": 4
        }
      ]
    }
  }
}