{
  "sampling_rate": 512,
  "channel_mapping": {
    "ch0": {
      "sensor": "EOG",
      "enabled": true
    },
    "ch1": {
      "sensor": "EEG",
      "enabled": true
    }
  },
  "filters": {
    "EMG": {
      "type": "high_pass",
      "cutoff": 70,
      "order": 4
    },
    "EOG": {
      "type": "low_pass",
      "cutoff": 10,
      "order": 4
    },
    "EEG": {
      "filters": [
        {
          "type": "notch",
          "freq": 50,
          "Q": 30
        },
        {
          "type": "bandpass",
          "low": 0.5,
          "high": 45,
          "order": 4
        }
      ]
    }
  },
  "display": {
    "timeWindowMs": 30000,
    "showGrid": true,
    "scannerX": 0
  },
  "num_channels": 2,
  "features": {
    "EOG": {
      "blink": {},
      "Rest": {}
    },
    "EMG": {
      "Rock": {},
      "Rest": {}
    }
  },
  "baud": 230400,
  "serial_port": "",
  "binary": true
}