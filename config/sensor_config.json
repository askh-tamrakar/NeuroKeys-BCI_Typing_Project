{
  "channel_mapping": {
    "ch0": {
      "sensor": "EMG",
      "enabled": true
    },
    "ch1": {
      "sensor": "EOG",
      "enabled": true
    }
  },
  "display": {
    "scannerX": 0,
    "showGrid": true,
    "timeWindowMs": 12000
  },
  "filters": {
    "EEG": {
      "filters": [
        {
          "Q": 30,
          "freq": 50,
          "type": "notch"
        },
        {
          "high": 45,
          "low": 0.5,
          "order": 4,
          "type": "bandpass"
        }
      ]
    },
    "EMG": {
      "cutoff": 70,
      "order": 4,
      "type": "high_pass"
    },
    "EOG": {
      "type": "bandpass",
      "low": 0.5,
      "high": 10.0,
      "order": 4
    },
    "bandpass": {
      "enabled": true
    },
    "notch": {
      "enabled": false
    }
  },
  "features": {
    "EOG": {
      "amp_threshold": 0.8,
      "min_duration_ms": 100.0,
      "max_duration_ms": 600.0,
      "min_asymmetry": 0.05,
      "max_asymmetry": 2.5,
      "min_kurtosis": -3.0
    }
  },
  "num_channels": 2,
  "sampling_rate": 512
}