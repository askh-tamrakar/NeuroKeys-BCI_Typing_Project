{
  "sampling_rate": 512,
  "channel_mapping": {
    "ch0": {
      "sensor": "EMG",
      "enabled": true
    },
    "ch1": {
      "sensor": "EOG",
      "enabled": true
    }
  },
  "filters": {
    "EMG": {
      "type": "high_pass",
      "cutoff": 70,
      "order": 4
    },
    "EOG": {
      "type": "low_pass",
      "cutoff": 10,
      "order": 4
    },
    "EEG": {
      "filters": [
        {
          "type": "notch",
          "freq": 50,
          "Q": 30
        },
        {
          "type": "bandpass",
          "low": 0.5,
          "high": 45,
          "order": 4
        }
      ]
    },
    "notch": {
      "enabled": false
    },
    "bandpass": {
      "enabled": true
    }
  },
  "display": {
    "timeWindowMs": 15000,
    "showGrid": true,
    "scannerX": 0
  },
  "num_channels": 2
}